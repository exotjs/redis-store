{
  "name": "@exotjs/redis-store",
  "version": "0.1.1",
  "description": "Redis store for Exot Inspector",
  "author": "Daniel Regeci",
  "license": "MIT",
  "keywords": [
    "exot",
    "inspector",
    "redis",
    "store"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types"
      ]
    }
  },
  "peerDependencies": {
    "@exotjs/measurements": "0.x"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "ioredis": "^5.3.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.1"
  }
}
